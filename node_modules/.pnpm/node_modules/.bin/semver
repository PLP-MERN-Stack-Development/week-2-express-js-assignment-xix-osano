#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/enosh/Documents/Programming/PLP FEB 2025 COHORT/Full-Stack MERN/Week 2/week-2-express-js-assignment-xix-osano/node_modules/.pnpm/semver@7.7.2/node_modules/semver/bin/node_modules:/home/enosh/Documents/Programming/PLP FEB 2025 COHORT/Full-Stack MERN/Week 2/week-2-express-js-assignment-xix-osano/node_modules/.pnpm/semver@7.7.2/node_modules/semver/node_modules:/home/enosh/Documents/Programming/PLP FEB 2025 COHORT/Full-Stack MERN/Week 2/week-2-express-js-assignment-xix-osano/node_modules/.pnpm/semver@7.7.2/node_modules:/home/enosh/Documents/Programming/PLP FEB 2025 COHORT/Full-Stack MERN/Week 2/week-2-express-js-assignment-xix-osano/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/enosh/Documents/Programming/PLP FEB 2025 COHORT/Full-Stack MERN/Week 2/week-2-express-js-assignment-xix-osano/node_modules/.pnpm/semver@7.7.2/node_modules/semver/bin/node_modules:/home/enosh/Documents/Programming/PLP FEB 2025 COHORT/Full-Stack MERN/Week 2/week-2-express-js-assignment-xix-osano/node_modules/.pnpm/semver@7.7.2/node_modules/semver/node_modules:/home/enosh/Documents/Programming/PLP FEB 2025 COHORT/Full-Stack MERN/Week 2/week-2-express-js-assignment-xix-osano/node_modules/.pnpm/semver@7.7.2/node_modules:/home/enosh/Documents/Programming/PLP FEB 2025 COHORT/Full-Stack MERN/Week 2/week-2-express-js-assignment-xix-osano/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../semver/bin/semver.js" "$@"
else
  exec node  "$basedir/../semver/bin/semver.js" "$@"
fi
